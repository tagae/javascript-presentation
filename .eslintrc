{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "strict": [2, "never"],
    "quotes": 0,
    "no-console": 0,
    "no-shadow": 1
  }
}
